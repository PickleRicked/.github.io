<addons>
<addon id="plugin.program.picklerickwizard" name=" [B][COLOR lime]PickleRick Wizard[/COLOR][/B]" version="1.6" provider-name="[B][COLOR blue]PickleRick[/COLOR][/B]">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">PickleRick Wizard</summary>
    <description lang="en">This be the tool ya'll need to get dem builds ya hear. Thanks whufclee at NaN, GUYS at TI, Brackets for the app installer!! RomanVM for the PyXBMCt libs. Fire TV Guru for the PyXBMCt layout  Also a very special Thanks to Surfacingx for the code used in this add-on.</description>
    <platform>all</platform>
  </extension>
</addon>

